@model Library.Pages.Copies.IndexModel

@{
    ViewData["Title"] = "Copies";
}

<h2>Copies</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                <!--  @Html.DisplayNameFor(model => model.Copy[0].Isbn) --> ISBN
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Copy[0].IsbnNavigation.Author)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Copy[0].IsbnNavigation.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Copy[0].Taken)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Copy[0].Return)
            </th>
