    <tbody>
@foreach (var item in Model.Copy) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Isbn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsbnNavigation.Author)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsbnNavigation.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Taken)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Return)
            </td>
            @*<td>
           @Html.DisplayFor(modelItem => item.IsbnNavigation.Author)
        </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.LibuserNavigation.Identitycode)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
